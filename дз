pip install matplotlib
import matplotlib.pyplot as plt
import numpy as np

# Згенеруємо значення x на відрізку від -3 до 3
x = np.linspace(-3, 3, 400)

# Обчислимо відповідні значення y за функцією y = -x^2 - 2x + 3
y = -x**2 - 2*x + 3

# Побудуємо графік
plt.plot(x, y, label='y = -x^2 - 2x + 3')
plt.xlabel('x')
plt.ylabel('y')
plt.title('Графік функції y = -x^2 - 2x + 3')
plt.grid(True)
plt.legend()
plt.show()
